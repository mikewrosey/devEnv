{
	"rules": {
		"class-name": true,
		"no-duplicate-variable": true,
		"no-any": true,
		"semicolon": [
			true,
			"always"
		],
		"max-line-length": {
			"options": [
				120
			]
		},
		"new-parens": true,
		"no-arg": true,
		"no-bitwise": true,
		"no-conditional-assignment": true,
		"no-consecutive-blank-lines": false,
		"no-console": {
			"options": [
				"debug",
				"info",
				"log",
				"time",
				"timeEnd",
				"trace"
			]
		}
	},
	"jsRules": {
		"max-line-length": {
			"options": [
				120
			]
		}
	}
}
